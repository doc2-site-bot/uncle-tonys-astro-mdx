{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@fragments/*": ["src/content/fragments/*"],
      "@layouts/*": ["src/layouts/*"],
      "@stores/*": ["src/stores/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
